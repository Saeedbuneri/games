{
  "buildCommand": null,
  "outputDirectory": ".",
  "framework": null,
  "devCommand": null,
  "installCommand": null,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Permissions-Policy",
          "value": "accelerometer=*, gyroscope=*, magnetometer=*"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/start.html",
      "permanent": false
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false
}
